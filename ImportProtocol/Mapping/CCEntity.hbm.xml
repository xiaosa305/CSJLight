<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
      assembly="ImportProtocol" namespace="ImportProtocol.Entity">
  <class name="CCEntity"  table="protocol_info">
      <id name="CCIndex" column="id">
        <generator class="assigned"></generator>
      </id>
      <property name="ProtocolName" column="protocol_name" not-null="true"/>
      <property name="Com0" column="com_zero"  not-null="true"/>
      <property name="Com1" column="com_one"  not-null="true"/>
      <property name="PS2" column="ps_two"  not-null="true"/>
  </class>
</hibernate-mapping>